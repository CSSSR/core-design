{
  "compilerOptions": {
    "declaration": true,
    "declarationDir": "./lib/typings",
    "declarationMap": true,
    "jsx": "preserve",
    "module": "commonjs",
    "target": "es5",
    "allowSyntheticDefaultImports": true,
    "lib": ["es6", "dom"],
    "noImplicitAny": false,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./lib",
    "baseUrl": "./"
  },
  "include": ["./src"]
}
