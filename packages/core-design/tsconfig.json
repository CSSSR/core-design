{
  "compilerOptions": {
    "declaration": true,
    "declarationDir": "./lib/typings",
    "declarationMap": true,
    "jsx": "preserve",
    "module": "commonjs",
    "target": "es5",
    "lib": ["es6", "dom"],
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./lib",
    "plugins": [
      {
        "name": "typescript-styled-plugin"
      }
    ]
  },
  "include": ["./src"]
}
